function getTranslate(targetLanguage, translationKey, defaultValue = "") {
  // Check if target language exists
  if (translations_js[targetLanguage]) {
    // Check if the translationKey exists for that language, otherwise return the defaultValue
    return translations_js[targetLanguage][translationKey] || defaultValue;
  } else {
    // Return defaultValue if the language is not found
    return defaultValue;
  }
}
